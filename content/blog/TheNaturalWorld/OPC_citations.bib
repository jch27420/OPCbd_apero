@article{werner_biosynthesis_2004,
	title = {Biosynthesis of {Gallic} {Acid} in {Rhus} {Typhina}: {Discrimination} between {Alternative} {Pathways} from {Natural} {Oxygen} {Isotope} {Abundance}},
	volume = {65},
	issn = {0031-9422},
