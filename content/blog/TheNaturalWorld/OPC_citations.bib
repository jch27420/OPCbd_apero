	note = {\_eprint: 22140654
\_eprinttype: pmid},
	keywords = {Male, Humans, Cell Proliferation, Digoxin, Cell Line, risk, Follow-Up Studies, Prospective Studies, Tumor, cohort, Cohort Studies, cytotoxicity, prostate cancer, Prostatic Neoplasms, Risk Factors, Surveys and Questionnaires, transdisciplinary},
	pages = {68--77},
