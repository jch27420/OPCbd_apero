	keywords = {Humans, Binding Sites, Caco-2 Cells, Anthocyanins, Polyphenols, Protein Structure, Secondary, Sirtuins, Histone Deacetylase Inhibitors, Molecular Docking Simulation},
	pages = {4163},
}

