	pages = {483--495},
}

@article{byvoet_detection_1995,
	title = {Detection of {Hydroxyl} {Radicals} upon {Interaction} of {Ozone} with {Aqueous} {Media} or {Extracellular} {Surfactant}: {The} {Role} of {Trace} {Iron}},
	volume = {319},
