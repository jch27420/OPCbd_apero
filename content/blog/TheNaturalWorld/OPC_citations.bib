	note = {Place: United States},
	keywords = {Animals, Chromatography, High Pressure Liquid, Amino Acid Sequence, Cattle, Mass Spectrometry, Molecular Weight, Chromatography, High Pressure Liquid, Carrier Proteins/*analysis, Retina/*analysis, Retinaldehyde/metabolism},
	pages = {18678--18687},
	annote = {Place: United States},
}

@article{crabb_cloning_1988,
