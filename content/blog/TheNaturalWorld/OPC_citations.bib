	keywords = {Phylogeny, Ecosystem, Carbon, Archaea, Acetyl Coenzyme A, Autotrophic Processes, Metabolic Networks and Pathways, Dicarboxylic Acid Transporters, Glucose, Hydroxybutyrates},
	pages = {447--460},
