}

@article{bekele_blend_2001,
	title = {Blend {Effects} in the {Toxicity} of the {Essential} {Oil} {Constituents} of {Ocimum} {Kilimandscharicum} and {Ocimum} {Kenyense} ({Labiateae}) on {Two} {Post}-{Harvest} {Insect} {Pests}},
