}

@article{rawlings_merops_2018,
	title = {The {MEROPS} {Database} of {Proteolytic} {Enzymes}, {Their} {Substrates} and {Inhibitors} in 2017 and a {Comparison} with {Peptidases} in the {PANTHER} {Database}},
