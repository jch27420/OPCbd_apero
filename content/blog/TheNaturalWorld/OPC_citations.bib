	note = {\_eprint: 26580629
\_eprinttype: pmid},
	keywords = {cysteine-rich peptides, cystine knot, defensin, hevein, knottin, plant antimicrobial peptides, thionin},
	pages = {711--757},
	annote = {\_eprint: 26580629 \_eprinttype: pmid},
	annote = {\_eprint: 26580629 \_eprinttype: pmid},
	annote = {\_eprint: 26580629 \_eprinttype: pmid},
}

