	journal = {Proceedings of the National Academy of Sciences},
	author = {Cao, Kun and van Lent, Richard and Kleyn, Aart W. and Kurahashi, Mitsunori and Juurlink, Ludo B. F.},
	month = jul,
	year = {2019},
	note = {Publisher: Proceedings of the National Academy of Sciences},
	pages = {13862--13866},
}

