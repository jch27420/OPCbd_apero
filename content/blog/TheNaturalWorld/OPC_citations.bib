	year = {2017},
	pmid = {28768713},
	keywords = {Humans, Energy Metabolism, Oxidative Stress, Thermodynamics, Oxygen, reactive oxygen species, oxidative stress, Singlet Oxygen, free energy of activation, singlet oxygen, alpha-Crystallins, alpha-Synuclein, Alzheimer Disease, Amyloid beta-Peptides, Cataract, Diabetes Mellitus, Type 2, Islet Amyloid Polypeptide, neurodegenerative disorders, Parkinson Disease, tau Proteins},
	pages = {2713--2731},
}

