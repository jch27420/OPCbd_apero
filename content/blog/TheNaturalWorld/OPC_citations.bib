@article{price_structure-function_1988,
	title = {Structure-{Function} {Relationships} in the {Na},{K}-{ATPase} {Alpha} {Subunit}: {Site}-{Directed} {Mutagenesis} of {Glutamine}-111 to {Arginine} and {Asparagine}-122 to {Aspartic} {Acid} {Generates} a {Ouabain}-{Resistant} {Enzyme}},
