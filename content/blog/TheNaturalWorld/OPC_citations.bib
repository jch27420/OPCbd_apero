	pmid = {14530397},
	note = {Publisher: National Academy of Sciences
Section: Commentary},
	pages = {11931--11932},
	file = {Snapshot:C\:\\Users\\Jeffrey Howard\\Zotero\\storage\\XIZ5H5L8\\11931.html:text/html},
}

