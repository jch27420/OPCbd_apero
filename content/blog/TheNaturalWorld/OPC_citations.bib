\_eprinttype: pmid},
	keywords = {Chromatography, Plants, Mass, Spectrometry, Gene Expression Regulation, Plant, Genetically Modified, Tobacco, Alcohol Oxidoreductases, Electrospray Ionization, Escherichia coli, Gallic Acid, Juglans, Oxidation-Reduction, Reverse-Phase, Shikimic Acid},
	pages = {555--565},
