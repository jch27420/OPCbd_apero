@article{lefevre_evidence_2010,
	title = {Evidence for trans-generational medication in nature},
	volume = {13},
	copyright = {© 2010 Blackwell Publishing Ltd/CNRS},
	issn = {1461-0248},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1461-0248.2010.01537.x},
	doi = {10.1111/j.1461-0248.2010.01537.x},
	abstract = {Ecology Letters (2010) 13: 1485–1493 Abstract Parasites pose a serious threat to host fitness, and natural selection should favour host traits that reduce infection or disease symptoms. Here, we provide the first evidence of trans-generational medication, in which animals actively use medicine to mitigate disease in their offspring. We studied monarch butterflies and their virulent protozoan parasites, and found that neither caterpillars nor adult butterflies could cure themselves of disease. Instead, adult butterflies preferentially laid their eggs on toxic plants that reduced parasite growth and disease in their offspring caterpillars. It has often been suggested that sick animals may use medication to cure themselves of disease, but evidence for the use of medication in nature has so far been scarce. Our results provide evidence that infected animals may indeed use medicine as a defence against parasites, and that such medication may target an individual’s offspring rather than the individual itself.},
	language = {en},
	number = {12},
	urldate = {2024-05-10},
	journal = {Ecology Letters},
	author = {Lefèvre, Thierry and Oliver, Lindsay and Hunter, Mark D. and De Roode, Jacobus C.},
	year = {2010},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2010.01537.x},
	keywords = {Host–parasite ecology, monarch butterfly, Ophryocystis elektroscirrha, self-medication, tritrophic interactions},
	pages = {1485--1493},
}

