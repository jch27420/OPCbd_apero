@article{costanza_value_1997,
	title = {The {Value} of the {World}'s {Ecosystem} {Services} and {Natural} {Capital}},
	volume = {387},
	issn = {1476-4687},
	url = {https://www.nature.com/articles/387253a0},
	doi = {10.1038/387253a0},
	abstract = {The services of ecological systems and the natural capital stocks that produce them are critical to the functioning of the Earth's life-support system. They contribute to human welfare, both directly and indirectly, and therefore represent part of the total economic value of the planet. We have estimated the current economic value of 17 ecosystem services for 16 biomes, based on published studies and a few original calculations. For the entire biosphere, the value (most of which is outside the market) is estimated to be in the range of US{\textbackslash}16-54 trillion (1012) per year, with an average of US{\textbackslash}33 trillion per year. Because of the nature of the uncertainties, this must be considered a minimum estimate. Global gross national product total is around US\$18 trillion per year.},
	number = {6630},
	urldate = {2019-11-09},
	journal = {Nature},
	author = {Costanza, Robert and d' Arge, Ralph and de Groot, Rudolf and Farber, Stephen and Grasso, Monica and Hannon, Bruce and Limburg, Karin and Naeem, Shahid and O'Neill, Robert V. and Paruelo, Jose and Raskin, Robert G. and Sutton, Paul and van den Belt, Marjan},
	month = may,
	year = {1997},
	pages = {253--260},
}

